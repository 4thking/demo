<%= render 'filter' %>

  
<div class="container">
  <div class="center">
    <div class="row">
      <% @profiles.each do |profile| %>
        <div class="col-md-3">
          <div class="thumbnail">
            <%= link_to image_tag(profile.image.thumb.url), profile %>
            <div class="caption">
              <h3> <%= profile.user.username %> </h3> 
              <p> <%= "MMR: #{profile.mmr}" %> </p>
            </div>
          </div>
        </div>
      <% end %>
    </div>
  </div>
    <%= will_paginate @profile, renderer: BootstrapPagination::Rails %>
</div>






